@import '../stars/stars-template';

.movie-container {
  display: flex;
  flex-direction: column;
  padding: 0 22rpx;
  margin-bottom: 20rpx;
  float:left;
}

.movie-post {
  width: 200rpx;
  height: 270rpx;
  padding-bottom: 20rpx;
}

.movie-name {
  width: 200rpx;
  margin-bottom: 16rpx;
  font-size: 24rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}